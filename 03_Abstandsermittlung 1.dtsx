                    <property
                      dataType="System.String"
                      description="Der auszuführende SQL-Befehl."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select aus.*,
  row_number() over (partition by  rad order by rnr desc ) rnr_neu
from (
select   row_number() over (order by (select null)) rnr,
 erg.*
from (
SELECT 
  org_type,
  replace(address_position_ns,',','.') address_position_ns,
  replace(address_position_ew,',','.') address_position_ew,
  '''' + descr +  '''' descr,
  org_type_en,
  org_type_de,
  0 rad,
  '' ende
  FROM [combit_Recruiting2].[dbo].[dist_appl_comp]
 where abst &lt;= ? and rnr_appl = 2 
union
SELECT 
  1 org_type,
  replace(address_position_ns,',','.') address_position_ns,
  replace(address_position_ew,',','.') address_position_ew,
  '''' + 'Mittelpunkt' + '''' descr,
  'Center' org_type_en,
 'Mittelpunkt' org_type_de,
  5 rad,
  '' ende
  FROM [combit_Recruiting2].[dbo].[dist_appl_comp]
where rnr_appl = 2 and org_type = 2
) erg
) aus
order by rnr</property>
