 where abst &lt;= ? and rnr_appl = 2 
union
SELECT 
  1 org_type,
  replace(address_position_ns,',','.') address_position_ns,
  replace(address_position_ew,',','.') address_position_ew,
  '''' + 'Mittelpunkt' + '''' descr,
  'Center' org_type_en,
